#include <stdio.h>

// 간단한 enum 정의
enum Boolean {
    false, // 0
    true   // 1
};

int main() {
    // enum 상수 사용
    enum Boolean isTrue = true;  // "isTrue" 변수에 true 열거형 상수를 할당합니다.

    if (isTrue) {
        printf("isTrue는 참(True)입니다.\n");
    } else {
        printf("isTrue는 거짓(False)입니다.\n");
    }

    return 0; // 프로그램 종료
}
